{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SimSage Crawler (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Crawlers/bin/Debug/net8.0/Crawlers.dll",
      "args": [
        "/debug",
        "-crawler", "file",
        "-server", "https://test8.simsage.ai/api",
        "-org", "01995eba-e3b4-a11f-958b-db39190ec88b",
        "-kb", "01997b1c-3247-c92f-bd5c-7e8cf78fcf8e",
        "-sid", "c2f320d0-f5ba-37de-a409-78ceaafbaa09",
        "-source", "1",
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "justMyCode": true
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
