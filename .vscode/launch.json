{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SimSage Crawler (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Crawlers/bin/Debug/net8.0/Crawlers.dll",
      "args": [
        "/debug",
        "-crawler", "file",
        "-server", "https://youserver.simsage.ai/api",
        "-org", "c276f883-e0c8-43ae-9119-df8b7df9c574",
        "-kb", "46ff0c75-7938-492c-ab50-442496f5de51",
        "-sid", "",
        "-source", "1",
        "-aes", ""
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "justMyCode": true
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
