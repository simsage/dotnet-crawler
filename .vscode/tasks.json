{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-x32",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/dotnet.sln",
        "--no-incremental",
        "--arch", "x86",
        "/property:GenerateFullPaths=false",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Generates the executable for the project."
    },
    {
      "label": "build-release-x32",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/dotnet.sln",
        "--no-incremental",
        "--configuration Release",
        "--arch", "x86",
        "/property:GenerateFullPaths=false",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "detail": "Generates the executable for the project."
    },
    {
      "label": "build-release-x64",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/dotnet.sln",
        "--no-incremental",
        "--configuration Release",
        "/property:GenerateFullPaths=false",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "detail": "Generates the executable for the project."
    },
    {
      "label": "publish-x32",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/dotnet.sln",
        "/property:GenerateFullPaths=true",
        "--arch", "x86",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "${workspaceFolder}/dotnet.sln",
        "/property:GenerateFullPaths=true",
        "--arch", "x86",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
